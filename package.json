{
  "name": "grimoirejs-forward-shading",
  "version": "1.3.1",
  "description": "Forward lighting package for grimoire.js",
  "main": "./ref/index.js",
  "scripts": {
    "start": "webpack --watch --progress",
    "bundle": "webpack --progress --watch",
    "prepublish": "npm run build -- --prod",
    "build": "webpack --progress",
    "generate-expose": "cauldron generate-exposure --src ./src --dest ./src/index.ts --ts --main ./src/main.ts --dts ./ref",
    "generate-reference": "cauldron generate-reference --src ./src --dest ./src/index.ts --ts --main ./src/main.ts --dts ./ref"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GrimoireGL/grimoirejs-forward.git"
  },
  "keywords": [
    "grimoire"
  ],
  "author": "GrimoireGL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GrimoireGL/grimoirejs-forward/issues"
  },
  "homepage": "https://github.com/GrimoireGL/grimoirejs-forward#readme",
  "typings": "./ref/index.d.ts",
  "files": [
    "src",
    "register",
    "ref"
  ],
  "grimoire": {
    "isPlugin": true,
    "ts": {
      "lib": "./lib",
      "temp": "./lib-ts",
      "src": "./src",
      "es5": "./lib-es5"
    },
    "main": "index.ts",
    "test": {
      "defaultGlob": "**/*Test.js",
      "src": "./test",
      "temp": "./lib-test"
    },
    "out": {
      "es6": "./product/grimoire.es2016.js",
      "es5": "./product/grimoire.js"
    },
    "txt-exts": [
      ".txt",
      ".html",
      ".goml",
      ".sort"
    ],
    "doc": {
      "src": "./doc",
      "header": "./doc/header.md",
      "footer": "./doc/footer.md",
      "dest": "./README.md"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "grimoirejs-cauldron": "^2.3.1-beta7",
    "raw-loader": "^0.5.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.5",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-shell-plugin": "^0.5.0",
    "yargs": "^6.6.0"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "grimoirejs": "beta",
    "grimoirejs-fundamental": "latest",
    "grimoirejs-math": "beta"
  }
}
