{
  "name": "grimoirejs-forward-shading",
  "version": "1.1.1",
  "description": "Forward lighting package for grimoire.js",
  "main": "./lib/index.js",
  "scripts": {
    "test": "inquisitor",
    "start": "cauldron build --watch --server",
    "build": "cauldron build",
    "prepublish": "cauldron build --babelSeparated && inquisitor && npm run doc",
    "doc": "cauldron doc",
    "scaffold": "cauldron scaffold"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GrimoireGL/grimoirejs-forward.git"
  },
  "keywords": [
    "grimoire"
  ],
  "author": "GrimoireGL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GrimoireGL/grimoirejs-forward/issues"
  },
  "homepage": "https://github.com/GrimoireGL/grimoirejs-forward#readme",
  "typings": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib-es5"
  ],
  "grimoire": {
    "isPlugin": true,
    "ts": {
      "lib": "./lib",
      "temp": "./lib-ts",
      "src": "./src",
      "es5": "./lib-es5"
    },
    "main": "index.ts",
    "test": {
      "defaultGlob": "**/*Test.js",
      "src": "./test",
      "temp": "./lib-test"
    },
    "out": {
      "es6": "./product/grimoire.es2016.js",
      "es5": "./product/grimoire.js"
    },
    "txt-exts": [
      ".txt",
      ".html",
      ".goml",
      ".sort"
    ],
    "doc": {
      "src": "./doc",
      "header": "./doc/header.md",
      "footer": "./doc/footer.md",
      "dest": "./README.md"
    }
  },
  "devDependencies": {
    "grimoirejs-cauldron": "^1.5.13",
    "grimoirejs-inquisitor": "^1.0.3"
  },
  "dependencies": {
    "grimoirejs-fundamental": "^0.4.10",
    "grimoirejs-math": "^1.2.4",
    "typescript": "beta"
  }
}
